// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
int n;
cin,n;
int a[n];
  for(int i=0;i<n;i++){
    cin>>a[i];
}
int sum=0
for(int i=0;i<n;i++){

   sum+=a[i];
  cout<<"i:"<<i<<"a[i];"<<a[i]<<"sum"<<sum<<endl;
  }
  cout<<"total sum:"<<sum;
  
    return 0;
}
